{
  "cSpell.words": [
    "antialiased",
    "cellpadding",
    "cellspacing",
    "copyfiles",
    "fastify",
    "gitignore",
    "graphiql",
    "lpush",
    "lrange",
    "lspace",
    "mariadb",
    "nbsp",
    "ormconfig",
    "preheader",
    "resave",
    "rspace",
    "typeorm",
    "varchar"
  ],
  "files.autoSave": "afterDelay",
  "editor.fontSize": 14,
  "git.autofetch": true,
  "eslint.autoFixOnSave": true,
  "files.exclude": {
    "node_modules/": true,
    "**/._*": true,
    "dist": true
  },
  "prettier.singleQuote": true,
  "workbench.editor.enablePreview": false,
  "editor.formatOnSave": true,
  "tslint.autoFixOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always"
}
